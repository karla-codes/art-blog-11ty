<ul class="art-container">
  {% for post in collections.artPosts | reverse %}
  <li>
    <a href="{{ post.url }}">
      <img src="{{ post.data.image }}" alt="{{ altText }}" />
    </a>
    <span class="art-container-info"
      >{{ post.data.date | readableDate }}<br />{{ post.data.title }}</span
    >
  </li>
  {% endfor %}
</ul>
